{
  "name": "calculator",
  "version": "1.0.0",
  "description": "Calculator",
  "scripts": {
    "parcel:clean": "rm -rf .parcel-cache dist",
    "parcel:build": "parcel build index.html --dist-dir dist --public-url ./",
    "parcel:compile": "parcel index.html",
    "parcel:watch": "nodemon --watch src --watch index.html --ext scss,js,html --exec \"npm run parcel:build\"",
    "serve": "live-server dist",
    "dev": "npm-run-all parcel:clean --parallel parcel:watch serve"
  },
  "author": "Marc Diallo",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.15.4",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.15.4",
    "sass": "^1.89.2"
  },
  "dependencies": {
    "core-js": "^3.43.0",
    "regenerator-runtime": "^0.14.1"
  }
}
